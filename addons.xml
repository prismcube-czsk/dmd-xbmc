    <import addon="xbmc.python" version="1.0"/>
    <import addon="script.module.dmd-czech.common" version="0.2"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <platform>all</platform>
    <summary>CT iVysilani video-archive plugin</summary>
    <summary lang="cs">ČT iVysílání - archiv</summary>
    <description>
        Plugin to play TV video archive from CT iVysilani
